<template name="dynamicItem">
  <view class="d-list-item" data-articleid = '{{item.articleId}}' bindtap = "goArticle">
      <view class = "dynamic-msg">
          <view class="dynamic-msg-header">{{item.title}}</view>
          <view class="dynamic-msg-date">{{item.relaseDate}}</view>
          <view class="dynamic-msg-content" >
              <view style="" class="content-img">
                 <image class="img"src="http://citgroup.cn/{{item.images}}"></image>
              </view>
              <view class="content-describe">{{item.description}}</view>
          </view>
          <view class="dynamic-msg-cut"></view>
      </view>
  </view>
</template>